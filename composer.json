{
    "name": "diegocaprioli/laravel-helpers",
    "description": "Useful helper functions for your Laravel project",
    "keywords": ["laravel", "helpers", "helper", "functions"],
    "authors": [
        {
            "name": "Diego Caprioli",
            "email": "diegocaprioli@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.0",
        "laravelcollective/html": "^5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "files": [
            "src/Helpers/array-functions.php",
            "src/Helpers/string-functions.php",
            "src/Helpers/form-functions.php",
            "src/Helpers/collection-functions.php",
            "src/Helpers/numeric-functions.php",
            "src/Helpers/html-functions.php"
        ],
        "psr-4": {
            "DiegoCaprioli\\LaravelHelpers\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
